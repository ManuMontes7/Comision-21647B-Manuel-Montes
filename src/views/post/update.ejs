<% title='Actualizar post' %>
  <%- include('../base/estructura-head.ejs'); %>
    <h4>Modificar post : <%= post.titulo %>
    </h4>
    <form action="/post/update/<%= post.id %>" method="post">
      <input placeholder="Titulo" name="titulo" value="<%= post.titulo %>" />
      <input placeholder="" name="contenido" value="<%= post.contenido %>" />
      <input placeholder="URL de Imagen" name="imagen" value="<%= post.imagen %>" />
      <button type="submit">Guardar</button>
    </form>
    <%- include('../base/estructura-foot.ejs'); %>